<!DOCTYPE html>
<html>
<head>
    <link rel="stylesheet" href="https://unpkg.com/leaflet@1.7.1/dist/leaflet.css" />
    <script src="https://unpkg.com/leaflet@1.7.1/dist/leaflet.js"></script>
    <style>
        #map {
            height: 400px;
        }
    </style>
</head>
<body>
    <div id="map"></div>

    <script>
        var map = L.map('map').setView([23.2599, 77.4126], 13); // Set the initial map view to Bhopal, Madhya Pradesh

        L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
            maxZoom: 19,
        }).addTo(map); // Add a tile layer from OpenStreetMap

        L.marker([23.2599, 77.4126]).addTo(map) // Add a marker to the map
            .bindPopup('This is Bhopal, Madhya Pradesh, India.'); // Add a popup message when you click on the marker
    </script>
</body>
</html>
